{
  "metadata": {
    "name": "Rapid Test Playbook",
    "taskNameTemplate": "${playbook_name} (${playbook_datetime}) ${distribution_count}",
    "labels": [
      "label1", "label2", "label3", "label4", "label5",
      "label6", "label7", "label8", "label9", "label10"
    ]
  },
  "workspace": {
    "source": {
      "artifactId": "logs11-140",
      "sourceType": "artifact"
    },
    "dependencies": [
      {
        "label": "dependency1",
        "source": {
          "sourceType": "artifact",
          "artifactId": "logs11-140"
        },
        "copyTo": "/invalid/path/../etc/passwd"  // Testing directory traversal handling
      }
    ]
  },
  "execution": {
    "testcases": [
      {
        "relativePath": "Commands/InitCommand.sh",
        "type": "command",
        "parameters": {
          "param1": "value1"
        }
      },
      {
        "relativePath": "Packages/MainTest.pkg",
        "type": "package"
      },
      {
        "relativePath": "Commands/CleanupCommand.sh",
        "type": "command"
      }
    ]
  },
  "noImplicitEcuTestStartStop": true,
  "xilConfigRequirements": [
    {
      "category": "performance",
      "label": "cpu",
      "value": "high"
    }
  ],
  "parameters": [
    {
      "name": "big_param",
      "value": "A".repeat(5000)  // Large parameter to test backend handling
    }
  ],
  "additionalSettings": {
    "analysisExecutionMode": "sequential",
    "downstreamAnalysisPartitioning": "analysis"
  }
}
